{
  "name": "teste-tecnico-back",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "npx dotenv -e .development.env nodemon src/server.ts",
    "start:dev": "npx dotenv -e .development.env node dist/src/server.js",
    "start:production": "npm run prisma:migrate:deploy && node dist/src/server.js",
    "test": "npx dotenv -e .test.env jest",
    "prisma:migrate:test": "npx dotenv -e .test.env npx prisma migrate dev",
    "prisma:migrate:dev": "npx dotenv -e .development.env npx prisma migrate dev",
    "prisma:migrate:deploy": "npx dotenv -e .env npx prisma migrate deploy",
    "prisma:generate:deploy": "npx prisma generate deploy",
    "prisma:generate:test": "npx dotenv -e .test.env npx prisma generate dev",
    "prisma:generate:dev": "npx dotenv -e .development.env npx prisma generate dev"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
